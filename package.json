{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tailwind build -i ./src/css/tailwind.css -o ./src/css/index.css --minify && vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "css": "tailwind build -i ./src/css/tailwind.css -o ./src/css/index.css --minify --watch",
    "format": "prettier --write \"src/**/*.{js,vue,ts}\""
  },
  "dependencies": {
    "@vue/eslint-config-prettier": "^9.0.0",
    "ant-design-vue": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}
